{
  "daemon": "onemetre_red_focuser",
  "log_name": "focusd@onemetre",
  "control_machines": ["OneMetreTCS", "OneMetreDome"],
  "serial_port": "/dev/focuser",
  "serial_baud": 9600,
  "serial_timeout": 5,
  "nominal_focus_position": 9000,
  "max_position": 50000,
  "moving_loop_delay": 0.5,
  "move_timeout": 180
}
